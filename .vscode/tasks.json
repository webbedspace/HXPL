{
    "version": "2.0.0",
    "command": "haxelib",
    "args": ["run", "munit", "test", "test-no-backend.hxml", "-coverage"],
    "taskName": "Massive Unit",
    "isBuildCommand": true,
    "options": {
        "cwd": "${workspaceRoot}/tests"
    },
    "problemMatcher": {
        "owner": "haxe",
        "fileLocation": ["relative", "${workspaceRoot}"],
        "pattern":{
            "regexp": "(haxepunk/[^\\.]+\\.hx):(\\d+): characters ([0-9]+)-([0-9]+) : (.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "endColumn": 4,
            "message": 5
        }
    },
    "tasks": [
        {
            "label": "haxelib",
            "type": "shell",
            "command": "haxelib",
            "args": [
                "run",
                "munit",
                "test",
                "test-no-backend.hxml",
                "-coverage"
            ],
            "problemMatcher": {
                "owner": "haxe",
                "fileLocation": [
                    "relative",
                    "${workspaceRoot}"
                ],
                "pattern": {
                    "regexp": "(haxepunk/[^\\.]+\\.hx):(\\d+): characters ([0-9]+)-([0-9]+) : (.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "endColumn": 4,
                    "message": 5
                }
            },
            "group": {
                "_id": "build",
                "isDefault": false
            }
        }
    ]
}